    "propertyDescriptions": {
      "sample_id": "The HuBMAP ID for the sample assigned by the ingest portal. Example: HBM743.CKJW.876",
      "source_storage_duration_value": "How long was the source material (parent) stored, prior to this sample being processed.",
      "weight_value": "The weight of a tissue block or the piece of tissue used in a suspension.",
      "volume_value": "The volume of the sample block",
      "pathology_distance_value": "If a surgical sample, how far from the pathology was the sample obtained.",
      "processing_time_value": "How long the tissue was being handled before the initial preservation",
      "quality_criteria": "For example, RIN: 8.7. For suspensions, measured by visual inspection prior to cell lysis or defined by known parameters such as wells with several cells or no cells. This can be captured at a high level. \"OK\" or \"not OK\", or with more specificity such as \"debris\", \"clump\", \"low clump\".",
      "histological_report": "Histopathological reporting of key variables that are important for the tissue (absence of necrosis, comment on composition, significant pathology description, high level inflammation/fibrosis assessment, etc.)",
      "notes": "Miscellaneous details about the sample, not captured in the existing metadata fields.",
      "metadata_schema_id": "The string that serves as the definitive identifier for the metadata schema version and is readily interpretable by computers for data validation and processing.",
      "header_info": "",
      "source_storage_duration_unit": "The time duration unit of measurement",
      "weight_unit": "The tissue weight unit of measurement",
      "volume_unit": "The volume unit of measurement",
      "pathology_distance_unit": "The distance unit of measurement",
      "preparation_medium": "The medium used during the sample preparation",
      "preparation_condition": "The condition by which the preparation occurred, such as was the sample placed in dry ice during the preparation.",
      "processing_time_unit": "The time unit of measurement",
      "storage_medium": "What was the sample preserved in.",
      "storage_method": "The method by which the sample was stored, after preparation and before the assay was performed."
    }
  },
